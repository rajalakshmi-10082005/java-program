import java.util.Scanner;

public class SumPositiveSquares {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();       
        long sum = 0L;
        for (int i = 0; i < n; i++) {
            int x = sc.nextInt();
            if (x > 0) {
                sum += (long) x * x;
            }
        }
        System.out.println(sum);
        sc.close();
    }
}
